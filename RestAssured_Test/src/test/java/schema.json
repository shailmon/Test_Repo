{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "https://example.com/object1617105750.json", 
	"title": "Root", 
	"type": "object",
	"required": [
		"books"
	],
	"properties": {
		"books": {
			"$id": "#root/books", 
			"title": "Books", 
			"type": "array",
			"default": [],
			"items":{
				"$id": "#root/books/items", 
				"title": "Items", 
				"type": "object",
				"required": [
					"isbn",
					"title",
					"subTitle",
					"author",
					"publish_date",
					"publisher",
					"pages",
					"description",
					"website"
				],
				"properties": {
					"isbn": {
						"$id": "#root/books/items/isbn", 
						"title": "Isbn", 
						"type": "string",
						"default": "",
						"examples": [
							"9781449325862"
						],
						"pattern": "^.*$"
					},
					"title": {
						"$id": "#root/books/items/title", 
						"title": "Title", 
						"type": "string",
						"default": "",
						"examples": [
							"Git Pocket Guide"
						],
						"pattern": "^.*$"
					},
					"subTitle": {
						"$id": "#root/books/items/subTitle", 
						"title": "Subtitle", 
						"type": "string",
						"default": "",
						"examples": [
							"A Working Introduction"
						],
						"pattern": "^.*$"
					},
					"author": {
						"$id": "#root/books/items/author", 
						"title": "Author", 
						"type": "string",
						"default": "",
						"examples": [
							"Richard E. Silverman"
						],
						"pattern": "^.*$"
					},
					"publish_date": {
						"$id": "#root/books/items/publish_date", 
						"title": "Publish_date", 
						"type": "string",
						"default": "",
						"examples": [
							"2020-06-04T08:48:39.000Z"
						],
						"pattern": "^.*$"
					},
					"publisher": {
						"$id": "#root/books/items/publisher", 
						"title": "Publisher", 
						"type": "string",
						"default": "",
						"examples": [
							"O'Reilly Media"
						],
						"pattern": "^.*$"
					},
					"pages": {
						"$id": "#root/books/items/pages", 
						"title": "Pages", 
						"type": "integer",
						"examples": [
							234
						],
						"default": 0
					},
					"description": {
						"$id": "#root/books/items/description", 
						"title": "Description", 
						"type": "string",
						"default": "",
						"examples": [
							"This pocket guide is the perfect on-the-job companion to Git, the distributed version control system. It provides a compact, readable introduction to Git for new users, as well as a reference to common commands and procedures for those of you with Git exp"
						],
						"pattern": "^.*$"
					},
					"website": {
						"$id": "#root/books/items/website", 
						"title": "Website", 
						"type": "string",
						"default": "",
						"examples": [
							"http://chimera.labs.oreilly.com/books/1230000000561/index.html"
						],
						"pattern": "^.*$"
					}
				}
			}

		}
	}
}
